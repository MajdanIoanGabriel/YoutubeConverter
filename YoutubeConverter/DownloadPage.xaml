<Page x:Class="YoutubeConverter.DownloadPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:YoutubeConverter"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DownloadPage">

    <Grid>
        <Label x:Name="video_title" Content="Video title" HorizontalAlignment="Left" Margin="120,98,0,0" VerticalAlignment="Top"/>
        <Label x:Name="video_duration" Content="Duration" HorizontalAlignment="Left" Margin="120,139,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="video_bitrate" HorizontalAlignment="Left" Margin="482,102,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="video_size" Content="Size" HorizontalAlignment="Left" Margin="482,139,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.658,-0.655"/>

    </Grid>
</Page>
